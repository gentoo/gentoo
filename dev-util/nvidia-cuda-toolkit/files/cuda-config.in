#!/bin/bash

SUPPORT_GCC_VERSIONS_BY_CUDA="CUDA_SUPPORTED_GCC"

_print_help() {
	cat <<- EOF
	Usage:
		$(basename $0) [options]
	
		-s | --supported   Returns by current CUDA supported gcc versions
		-h | --help        Shows this help
	EOF
}

case ${1} in
	-s|--supported)
		echo "${SUPPORT_GCC_VERSIONS_BY_CUDA}"
		exit 0
		;;
	-h|--help)
		_print_help
		exit -1
		;;
	*)
		_print_help
		exit 1
		;;
esac
