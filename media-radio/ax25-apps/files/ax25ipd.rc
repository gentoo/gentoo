depend() {
	need net
}

start() {
	ebegin "Starting ax25ipd"
	start-stop-daemon --start --quiet --background --exec /usr/sbin/ax25ipd
	eend $?
}

stop() {
	ebegin "Stopping ax25ipd"
	start-stop-daemon --stop --quiet --exec /usr/sbin/ax25ipd
	eend $?
}
