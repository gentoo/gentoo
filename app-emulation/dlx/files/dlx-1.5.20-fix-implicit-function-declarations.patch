--- a/masm.c
+++ b/masm.c
@@ -51,6 +51,8 @@ extern  char    LinkDate [];
 
 /*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
 
+VOID Error (STRPTR Msg);
+
 FILE    *In;                    /* Input file */
 
 BOOL    LittleEndian = FALSE, Ok = TRUE, ESym = FALSE;
--- a/masm.h
+++ b/masm.h
@@ -13,5 +13,3 @@
 
