--- a/mon.c
+++ b/mon.c
@@ -88,6 +88,7 @@
 #include "mon.h"
 #include "io.h"
 
+extern void Error (STRPTR Msg, BOOL PrintLine);
 ULONG   LastAddr = 0;
 
 /*~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/
--- a/mon.h
