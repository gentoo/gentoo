@@ -46,6 +46,7 @@ extern  UBYTE   PauseForKey (UBYTE Msg);
 
 extern  ULONG   DebugLevel;
 extern  FILE    *Log;
+VOID DisMicrocode ();
 
 #define DB_DISASSEM     0x0001
