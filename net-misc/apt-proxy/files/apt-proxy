# default: off
# description: this is a proxy for debian packages.  
# $Id$

service apt-proxy
{
	type		= UNLISTED
	port            = 9999
	protocol	= tcp
	socket_type     = stream
	only_from	= 0.0.0.0
	wait            = no
	user            = apt-proxy
	server		= /usr/sbin/apt-proxy
	server_args	= /var/log/apt-proxy.log
	disable         = yes
}
