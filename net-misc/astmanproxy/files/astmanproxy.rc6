depend() {
	need net
}

start() {
	ebegin "Starting astmanproxy"
	start-stop-daemon --start --exec /usr/sbin/astmanproxy
	eend $?
}

stop() {
	ebegin "Stopping astmanproxy"
	start-stop-daemon --stop --exec /usr/sbin/astmanproxy
	eend $?
}
