<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "http://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
	<herd>python</herd>
	<upstream>
		<remote-id type="pypi">kombu</remote-id>
		<remote-id type="github">celery/kombu</remote-id>
	</upstream>
	<use>
