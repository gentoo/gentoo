<?xml version='1.0'?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-veusz">
    <comment>Veusz saved graph</comment>
    <glob pattern="*.vsz"/>
    <magic priority="50">
        <match type="string" value="# Veusz saved document" offset="0"/>
    </magic>
  </mime-type>
</mime-info>
